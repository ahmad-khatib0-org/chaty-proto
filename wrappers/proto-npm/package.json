{
  "name": "@chaty-app/proto",
  "version": "0.1.20",
  "description": "TypeScript Protobuf bindings for Chaty (Pure ESM)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@connectrpc/connect": "^2.0.0",
    "@connectrpc/connect-web": "^2.0.0",
    "@bufbuild/protobuf": "^2.2.3",
    "@grpc/grpc-js": "^1.13.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js",
    "./web/*": "./dist/web/*.js",
    "./node/*": "./dist/node/*.js"
  },
  "typesVersions": {
    "*": {
      "web/service/v1": [
        "dist/web/service/v1/index.d.ts"
      ],
      "web/service/v1/*": [
        "dist/web/service/v1/*.d.ts"
      ],
      "web/shared/v1": [
        "dist/web/shared/v1/index.d.ts"
      ],
      "web/shared/v1/*": [
        "dist/web/shared/v1/*.d.ts"
      ],
      "node/service/v1": [
        "dist/node/service/v1/index.d.ts"
      ],
      "node/service/v1/*": [
        "dist/node/service/v1/*.d.ts"
      ],
      "node/shared/v1": [
        "dist/node/shared/v1/index.d.ts"
      ],
      "node/shared/v1/*": [
        "dist/node/shared/v1/*.d.ts"
      ]
    }
  }
}
